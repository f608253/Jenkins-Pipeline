pipeline {
          agent any 
          stages {
                stage('Build') {
                     steps {
                          echo "Building the code stage"
                     }
                 }
                stage('Test') {
                     steps {
                           echo "Testing stage"
                     }
                }
                stage('Deploy') {
                     steps {
                          echo "Deploying stage of code"
                     }
                }
           }
}
